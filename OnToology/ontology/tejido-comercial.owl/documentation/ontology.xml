    <!-- http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial#agrupacionComercial -->


    <owl:ObjectProperty rdf:about="http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial#agrupacionComercial">
        <rdfs:range rdf:resource="http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial#AgrupacionComercial"/>
        <rdfs:comment xml:lang="en">Any entity (for instance, a local business premise) may belong to a shopping area</rdfs:comment>
        <rdfs:comment xml:lang="es">Cualquier entidad (por ejemplo, un local comercial) puede pertenecer a una agrupación comercial.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial"/>
        <rdfs:label xml:lang="en">belongs to shopping area</rdfs:label>
        <rdfs:label xml:lang="es">pertenece a agrupación comercial</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial#asociadaA -->


    <owl:ObjectProperty rdf:about="http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial#asociadaA">
        <owl:inverseOf rdf:resource="http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial#tieneLicenciaApertura"/>
        <rdfs:domain rdf:resource="http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial#LicenciaApertura"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial#LocalComercial"/>
                    <rdf:Description rdf:about="http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial#Terraza"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">An opening license is associated to a local business or terrace.</rdfs:comment>
        <rdfs:comment xml:lang="es">Una licencia de apertura está asociada a un local comercial o a una terraza.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial"/>
        <rdfs:label xml:lang="es">asociada a</rdfs:label>
        <rdfs:label xml:lang="en">associated to</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial#autorizaActividadEconomica -->


    <owl:ObjectProperty rdf:about="http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial#autorizaActividadEconomica">
        <rdfs:domain rdf:resource="http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial#LicenciaApertura"/>
        <rdfs:range rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <rdfs:comment xml:lang="en">A license authorizes one or several economic activities. The economic activity type is represented by some SKOS classification, preferably one of the following:
http://vocab.linkeddata.es/datosabiertos/kos/comercio/cnae
http://vocab.linkeddata.es/datosabiertos/kos/comercio/iae</rdfs:comment>
        <rdfs:comment xml:lang="es">Una licencia autoriza la realización de una o varias actividades económicas. El tipo de actividad económica que se autoriza se representa de acuerdo con alguna clasificación SKOS, preferentemente alguna de las siguientes:
http://vocab.linkeddata.es/datosabiertos/kos/comercio/cnae
http://vocab.linkeddata.es/datosabiertos/kos/comercio/iae</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial"/>
        <rdfs:label xml:lang="en">authorizes economic activity</rdfs:label>
        <rdfs:label xml:lang="es">autoriza actividad económica</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial#contieneLocalComercial -->


    <owl:ObjectProperty rdf:about="http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial#contieneLocalComercial">
        <rdfs:range rdf:resource="http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial#LocalComercial"/>
        <rdfs:comment xml:lang="en">An area (for instance, a shopping area) contains local business premises.</rdfs:comment>
        <rdfs:comment xml:lang="es">Una agrupación (por ejemplo, una agrupación comercial) contiene locales comerciales.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial"/>
        <rdfs:label xml:lang="en">contains local business premise</rdfs:label>
        <rdfs:label xml:lang="es">contiene local comercial</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial#estadoTramitacion -->


    <owl:ObjectProperty rdf:about="http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial#estadoTramitacion">
        <rdfs:domain rdf:resource="http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial#LicenciaApertura"/>
        <rdfs:range>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3.org/2004/02/skos/core#Concept"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.w3.org/2004/02/skos/core#inScheme"/>
                        <owl:hasValue rdf:resource="http://vocab.linkeddata.es/datosabiertos/kos/comercio/tipo-estado-tramite-licencia"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="es">Estado de tramitación de la licencia, se representa con la lista de conceptos SKOS http://vocab.linkeddata.es/datosabiertos/kos/comercio/tipo-estado-tramite-licencia.</rdfs:comment>
        <rdfs:comment xml:lang="en">License processing status, it is represented by the SKOS concept list http://vocab.linkeddata.es/datosabiertos/kos/comercio/tipo-estado-tramite-licencia.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial"/>
        <rdfs:label xml:lang="es">estado de tramitación</rdfs:label>
        <rdfs:label xml:lang="en">processing status</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial#periodoFuncionamiento -->


    <owl:ObjectProperty rdf:about="http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial#periodoFuncionamiento">
        <rdfs:domain rdf:resource="http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial#Terraza"/>
        <rdfs:range>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3.org/2004/02/skos/core#Concept"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.w3.org/2004/02/skos/core#inScheme"/>
                        <owl:hasValue rdf:resource="http://vocab.linkeddata.es/datosabiertos/kos/comercio/periodo-funcionamiento"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="es">Periodo de funcionamiento de la terraza, que puede ser anual o estacional. Los periodos se representan mediante la lista de conceptos http://vocab.linkeddata.es/datosabiertos/kos/comercio/periodo-funcionamiento.</rdfs:comment>
        <rdfs:comment xml:lang="en">Terrace operational period, which can be: annual or seasonal. Periods are represented by the concept list http://vocab.linkeddata.es/datosabiertos/kos/comercio/periodo-funcionamiento.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial"/>
        <rdfs:label xml:lang="en">operational time period</rdfs:label>
        <rdfs:label xml:lang="es">periodo de funcionamiento</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial#perteneceALocal -->


    <owl:ObjectProperty rdf:about="http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial#perteneceALocal">
        <owl:inverseOf rdf:resource="http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial#tieneTerraza"/>
        <rdfs:domain rdf:resource="http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial#Terraza"/>
        <rdfs:range rdf:resource="http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial#LocalComercial"/>
        <rdfs:comment xml:lang="en">A terrace belongs to a local business premise.</rdfs:comment>
        <rdfs:comment xml:lang="es">Una terraza pertenece a un local comercial.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial"/>
        <rdfs:label xml:lang="en">belongs to local</rdfs:label>
        <rdfs:label xml:lang="es">pertenece a local</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial#seOtorgaA -->


    <owl:ObjectProperty rdf:about="http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial#seOtorgaA">
        <rdfs:domain rdf:resource="http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial#LicenciaApertura"/>
        <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
        <rdfs:comment xml:lang="en">A license is granted to a holder.</rdfs:comment>
        <rdfs:comment xml:lang="es">Una licencia se otorga a un titular.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial"/>
        <rdfs:label xml:lang="en">is granted to</rdfs:label>
        <rdfs:label xml:lang="es">se otorga a</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial#tieneLicenciaApertura -->


    <owl:ObjectProperty rdf:about="http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial#tieneLicenciaApertura">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial#LocalComercial"/>
                    <rdf:Description rdf:about="http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial#Terraza"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial#LicenciaApertura"/>
        <rdfs:comment xml:lang="en">A local business premise may have one or several opening licenses in its history.</rdfs:comment>
        <rdfs:comment xml:lang="es">El local comercial puede tener una o varias licencias de funcionamiento a lo largo de su historia.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial"/>
        <rdfs:label xml:lang="en">has opening license</rdfs:label>
        <rdfs:label xml:lang="es">tiene licencia de apertura</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial#tieneTerraza -->


    <owl:ObjectProperty rdf:about="http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial#tieneTerraza">
        <rdfs:domain rdf:resource="http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial#LocalComercial"/>
        <rdfs:range rdf:resource="http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial#Terraza"/>
        <rdfs:comment xml:lang="en">A local business premise may have a terrace.</rdfs:comment>
        <rdfs:comment xml:lang="es">Un local comercial puede tener una terraza asociada.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial"/>
        <rdfs:label xml:lang="en">has terrace</rdfs:label>
        <rdfs:label xml:lang="es">tiene terraza</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial#tipoAcceso -->


    <owl:ObjectProperty rdf:about="http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial#tipoAcceso">
        <rdfs:domain rdf:resource="http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial#LocalComercial"/>
        <rdfs:range>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3.org/2004/02/skos/core#Concept"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.w3.org/2004/02/skos/core#inScheme"/>
                        <owl:hasValue rdf:resource="http://vocab.linkeddata.es/datosabiertos/kos/comercio/tipo-acceso"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">A local business may have the following access types: street door, interior, or floor. These access types are represented by a list of URI concepts http://vocab.linkeddata.es/datosabiertos/kos/comercio/tipo-acceso.</rdfs:comment>
        <rdfs:comment xml:lang="es">Un local comercial puede tener los siguientes tipos de acceso: puerta de calle, interior, o piso. Estos tipos de acceso se representan mediante una lista de conceptos de URI http://vocab.linkeddata.es/datosabiertos/kos/comercio/tipo-acceso.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial"/>
        <rdfs:label xml:lang="en">access type</rdfs:label>
        <rdfs:label xml:lang="es">tipo de acceso</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial#tipoActividadEconomica -->


    <owl:ObjectProperty rdf:about="http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial#tipoActividadEconomica">
        <rdfs:domain rdf:resource="http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial#LocalComercial"/>
        <rdfs:range rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <rdfs:comment xml:lang="en">Economic activity type that takes place in a local business premise, according to some SKOS classification.</rdfs:comment>
        <rdfs:comment xml:lang="es">Tipo de actividad económica que se realiza en un local comercial, de acuerdo con alguna clasificación SKOS.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial"/>
        <rdfs:label xml:lang="en">economic activity type</rdfs:label>
        <rdfs:label xml:lang="es">tipo de actividad económica</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial#tipoAgrupacionComercial -->


    <owl:ObjectProperty rdf:about="http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial#tipoAgrupacionComercial">
        <rdfs:domain rdf:resource="http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial#AgrupacionComercial"/>
        <rdfs:range>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3.org/2004/02/skos/core#Concept"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.w3.org/2004/02/skos/core#inScheme"/>
                        <owl:hasValue rdf:resource="http://vocab.linkeddata.es/datosabiertos/kos/comercio/tipo-agrupacion"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">Type of shopping area, from the concept list http://vocab.linkeddata.es/datosabiertos/kos/comercio/tipo-agrupacion.</rdfs:comment>
        <rdfs:comment>Tipo de agrupación comercial, de acuerdo con la lista de conceptos http://vocab.linkeddata.es/datosabiertos/kos/comercio/tipo-agrupacion.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://vocab.ciudadesabiertas.es/def/comercio/tejido-comercial"/>
